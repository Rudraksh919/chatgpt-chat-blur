{
  "manifest_version": 3,
  "name": "ChatGPT Chat Blur",
  "version": "1.0",
  "description": "Blurs ChatGPT chats individually",
  "permissions": [],
  "host_permissions": ["https://chat.openai.com/*", "https://chatgpt.com/*"],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*", "https://chatgpt.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
